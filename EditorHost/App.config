<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
      <services>
        <service name="TodoListLib.TodoEditor" behaviorConfiguration="mexBehavior">
          <endpoint address="TodoListLib.TodoEditor" binding="netTcpBinding" contract="TodoListLib.ITodoEditor"/>
          <endpoint address="mex" binding="mexTcpBinding" contract="IMetadataExchange"/>
          <host>
            <baseAddresses>
              <add baseAddress="net.tcp://localhost:1919/" />
            </baseAddresses>
          </host>
       </service>
      </services>
      <behaviors>
        <serviceBehaviors>
          <behavior name="mexBehavior">
            <serviceMetadata httpGetEnabled="false"/>
          </behavior>
          <behavior name="exc">
            <serviceDebug includeExceptionDetailInFaults="true"/>
          </behavior>
        </serviceBehaviors>
      </behaviors>
   </system.serviceModel>
</configuration>